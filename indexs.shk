int x = 2;
int y = 10;

function int subtract(int a, int b)
{
    return a - b;
}

int diff = subtract(y, x);

if (diff > 5) {
    int result = diff * 2;
    call(result, 1, 3);
}

if (diff <= 5) {
    while (x < 5) {
        x = x + 1;
    }
    
    int incremented = x * 3;
    call(incremented, 4, 6);
}

int total = 0;
int counter = 0;
while (counter < 4) {
    total = total + counter;
    counter = counter + 1;
}

call(total, 7, 9);
